      <command>SET BUILDRELEASE=false&#xd;
SET BUILDDEBUG=false&#xd;
SET AUTODELIVER=false&#xd;
&#xd;
SET BUILDVERSION=%BUILD_VERSION%&#xd;
python %team_scm_fetchDestination%\OMD_Builds\JenkinsBuild\VersionAutoIncremental\UpdateVersion.py %VERSIONFILE% %BUILDVERSION%</command>
    </hudson.tasks.BatchFile>
